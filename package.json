{
  "name": "crafter-default",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "performance:html": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "performance:all": "lighthouse http://localhost:3000 --output=json,html --output-path=./lighthouse-report",
    "bundle-analyzer": "next-bundle-analyzer",
    "build:prod": "NODE_ENV=production next build",
    "start:prod": "NODE_ENV=production next start",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@lexical/link": "^0.32.1",
    "@lexical/list": "^0.32.1",
    "@lexical/react": "^0.32.1",
    "@lexical/rich-text": "^0.32.1",
    "@lexical/selection": "^0.32.1",
    "@lexical/table": "^0.32.1",
    "@lexical/utils": "^0.32.1",
    "@next/third-parties": "^15.3.4",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.10",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.11",
    "axios": "^1.9.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.19.1",
    "lexical": "^0.32.1",
    "lucide-react": "^0.503.0",
    "next": "^15.3.4",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.0.0",
    "react-fast-marquee": "^1.6.5",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-turnstile": "^1.1.4",
    "satori": "^0.16.2",
    "sharp": "^0.34.3",
    "sweetalert2": "^11.6.13",
    "sweetalert2-react-content": "^5.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "lighthouse": "^12.7.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
